{"ast":null,"code":"import axios from 'axios';\nimport * as echarts from 'echarts';\n// 导入生命周期函数\nimport { onMounted } from 'vue';\nexport default {\n  name: 'ChinaMap',\n  setup() {\n    const getdata = () => {\n      // then请求成功，catch请求失败\n      axios.get('../json/china').then(res => {\n        // 输出矢量地图数据\n        // console.log(res.data);\n        var chinaCharts = echarts.init(document.getElementById('CN_Map'));\n        // 在axios的回调函数中注册地图矢量数据\n        echarts.registerMap('chinaMap', res.data);\n        var CNoption = {\n          geo: {\n            type: 'map',\n            map: 'chinaMap',\n            roam: true //允许缩放以及拖动效果\n          }\n        };\n\n        chinaCharts.setOption(CNoption);\n      }).catch(error => {\n        console.log(error);\n      });\n    };\n    // 调用，执行函数\n    onMounted(() => {\n      getdata();\n    });\n  }\n};","map":{"version":3,"names":["axios","echarts","onMounted","name","setup","getdata","get","then","res","chinaCharts","init","document","getElementById","registerMap","data","CNoption","geo","type","map","roam","setOption","catch","error","console","log"],"sources":["D:\\VUE_project\\data_screen\\src\\components\\ChinaMap.vue"],"sourcesContent":["<template>\r\n  <div id=\"CN_Map\" style=\"width: 600px;height: 600px;\"></div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport axios from 'axios';\r\nimport * as echarts from 'echarts'\r\n// 导入生命周期函数\r\nimport { onMounted } from 'vue';\r\nexport default {\r\n  name: 'ChinaMap',\r\n  setup() {\r\n    const getdata = () => {\r\n      // then请求成功，catch请求失败\r\n      axios.get('../json/china').then((res) => {\r\n        // 输出矢量地图数据\r\n        // console.log(res.data);\r\n        var chinaCharts = echarts.init(document.getElementById('CN_Map'));\r\n        // 在axios的回调函数中注册地图矢量数据\r\n        echarts.registerMap('chinaMap', res.data);\r\n        var CNoption = {\r\n          geo: {\r\n            type: 'map',\r\n            map: 'chinaMap',\r\n            roam: true  //允许缩放以及拖动效果\r\n          }\r\n        };\r\n        chinaCharts.setOption(CNoption);\r\n      }).catch((error) => {\r\n        console.log(error);\r\n      });\r\n    }\r\n    // 调用，执行函数\r\n    onMounted(() => {\r\n      getdata();\r\n    });\r\n\r\n  }\r\n}\r\n</script>\r\n\r\n<style></style>"],"mappings":"AAMA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAO,KAAKC,OAAM,MAAO,SAAQ;AACjC;AACA,SAASC,SAAQ,QAAS,KAAK;AAC/B,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,KAAKA,CAAA,EAAG;IACN,MAAMC,OAAM,GAAIA,CAAA,KAAM;MACpB;MACAL,KAAK,CAACM,GAAG,CAAC,eAAe,CAAC,CAACC,IAAI,CAAEC,GAAG,IAAK;QACvC;QACA;QACA,IAAIC,WAAU,GAAIR,OAAO,CAACS,IAAI,CAACC,QAAQ,CAACC,cAAc,CAAC,QAAQ,CAAC,CAAC;QACjE;QACAX,OAAO,CAACY,WAAW,CAAC,UAAU,EAAEL,GAAG,CAACM,IAAI,CAAC;QACzC,IAAIC,QAAO,GAAI;UACbC,GAAG,EAAE;YACHC,IAAI,EAAE,KAAK;YACXC,GAAG,EAAE,UAAU;YACfC,IAAI,EAAE,IAAG,CAAG;UACd;QACF,CAAC;;QACDV,WAAW,CAACW,SAAS,CAACL,QAAQ,CAAC;MACjC,CAAC,CAAC,CAACM,KAAK,CAAEC,KAAK,IAAK;QAClBC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC;MACpB,CAAC,CAAC;IACJ;IACA;IACApB,SAAS,CAAC,MAAM;MACdG,OAAO,CAAC,CAAC;IACX,CAAC,CAAC;EAEJ;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}