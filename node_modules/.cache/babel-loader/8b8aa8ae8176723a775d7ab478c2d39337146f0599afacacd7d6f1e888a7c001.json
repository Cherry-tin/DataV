{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\n// 导入生命周期函数\nimport { onMounted } from 'vue';\nimport axios from 'axios';\nimport * as echarts from 'echarts';\nexport default {\n  name: 'GuangDong',\n  setup() {\n    const gender = () => {\n      axios.get('http://localhost:8888/guangdong/list').then(gdong => {\n        // console.log(cam.data);\n        // 处理数据\n        const data = gdong.data;\n        var arrayx = []; //市名\n        var arrayy = []; //人数\n\n        // 分类数据\n        for (let item = 0; item < data.length; item++) {\n          arrayx.push(data[item].name);\n          arrayy.push(data[item].value);\n        }\n        var YueCharts = echarts.init(document.getElementById('yue'));\n        var Yueoption = {\n          tooltip: {\n            trigger: 'axis',\n            axisPointer: {\n              type: 'shadow'\n            }\n          },\n          grid: {\n            left: '3%',\n            right: '4%',\n            bottom: '3%',\n            containLabel: true\n          },\n          xAxis: [{\n            type: 'category',\n            data: arrayx,\n            axisTick: {\n              alignWithLabel: true\n            }\n          }],\n          yAxis: [{\n            type: 'value'\n          }],\n          series: [{\n            name: '人数',\n            type: 'bar',\n            barWidth: '60%',\n            data: arrayy\n          }]\n        };\n        YueCharts.setOption(Yueoption);\n      }).catch(error => {\n        console.log(error);\n      });\n    };\n    // 调用，执行函数\n    onMounted(() => {\n      gender();\n    });\n  }\n};","map":{"version":3,"names":["onMounted","axios","echarts","name","setup","gender","get","then","gdong","data","arrayx","arrayy","item","length","push","value","YueCharts","init","document","getElementById","Yueoption","tooltip","trigger","axisPointer","type","grid","left","right","bottom","containLabel","xAxis","axisTick","alignWithLabel","yAxis","series","barWidth","setOption","catch","error","console","log"],"sources":["D:\\VUE_project\\data_screen\\src\\components\\Yue.vue"],"sourcesContent":["<template>\r\n    <div id=\"yue\"></div>\r\n</template>\r\n\r\n<script>\r\n// 导入生命周期函数\r\nimport { onMounted } from 'vue';\r\nimport axios from 'axios';\r\nimport * as echarts from 'echarts'\r\n\r\nexport default {\r\n\r\n    name: 'GuangDong',\r\n    setup() {\r\n        const gender = () => {\r\n            axios.get('http://localhost:8888/guangdong/list').then((gdong) => {\r\n                // console.log(cam.data);\r\n                // 处理数据\r\n                const data = gdong.data;\r\n\r\n                var arrayx = [];//市名\r\n                var arrayy = [];//人数\r\n\r\n                // 分类数据\r\n                for (let item = 0; item < data.length; item++) {\r\n                    arrayx.push(data[item].name);\r\n                    arrayy.push(data[item].value);\r\n                }\r\n\r\n                var YueCharts = echarts.init(document.getElementById('yue'));\r\n                var Yueoption = {\r\n                    tooltip: {\r\n                        trigger: 'axis',\r\n                        axisPointer: {\r\n                            type: 'shadow'\r\n                        }\r\n                    },\r\n                    grid: {\r\n                        left: '3%',\r\n                        right: '4%',\r\n                        bottom: '3%',\r\n                        containLabel: true\r\n                    },\r\n                    xAxis: [\r\n                        {\r\n                            type: 'category',\r\n                            data: arrayx,\r\n                            axisTick: {\r\n                                alignWithLabel: true\r\n                            }\r\n                        }\r\n                    ],\r\n                    yAxis: [\r\n                        {\r\n                            type: 'value'\r\n                        }\r\n                    ],\r\n                    series: [\r\n                        {\r\n                            name: '人数',\r\n                            type: 'bar',\r\n                            barWidth: '60%',\r\n                            data: arrayy\r\n                        }\r\n                    ]\r\n                };\r\n                YueCharts.setOption(Yueoption);\r\n            }).catch((error) => {\r\n                console.log(error);\r\n            })\r\n\r\n        }\r\n        // 调用，执行函数\r\n        onMounted(() => {\r\n            gender();\r\n        });\r\n    }\r\n\r\n}\r\n</script>\r\n\r\n<style>\r\n#yue {\r\n    width: 500px;\r\n    height: 800px;\r\n}\r\n</style>"],"mappings":";AAKA;AACA,SAASA,SAAQ,QAAS,KAAK;AAC/B,OAAOC,KAAI,MAAO,OAAO;AACzB,OAAO,KAAKC,OAAM,MAAO,SAAQ;AAEjC,eAAe;EAEXC,IAAI,EAAE,WAAW;EACjBC,KAAKA,CAAA,EAAG;IACJ,MAAMC,MAAK,GAAIA,CAAA,KAAM;MACjBJ,KAAK,CAACK,GAAG,CAAC,sCAAsC,CAAC,CAACC,IAAI,CAAEC,KAAK,IAAK;QAC9D;QACA;QACA,MAAMC,IAAG,GAAID,KAAK,CAACC,IAAI;QAEvB,IAAIC,MAAK,GAAI,EAAE,CAAC;QAChB,IAAIC,MAAK,GAAI,EAAE,CAAC;;QAEhB;QACA,KAAK,IAAIC,IAAG,GAAI,CAAC,EAAEA,IAAG,GAAIH,IAAI,CAACI,MAAM,EAAED,IAAI,EAAE,EAAE;UAC3CF,MAAM,CAACI,IAAI,CAACL,IAAI,CAACG,IAAI,CAAC,CAACT,IAAI,CAAC;UAC5BQ,MAAM,CAACG,IAAI,CAACL,IAAI,CAACG,IAAI,CAAC,CAACG,KAAK,CAAC;QACjC;QAEA,IAAIC,SAAQ,GAAId,OAAO,CAACe,IAAI,CAACC,QAAQ,CAACC,cAAc,CAAC,KAAK,CAAC,CAAC;QAC5D,IAAIC,SAAQ,GAAI;UACZC,OAAO,EAAE;YACLC,OAAO,EAAE,MAAM;YACfC,WAAW,EAAE;cACTC,IAAI,EAAE;YACV;UACJ,CAAC;UACDC,IAAI,EAAE;YACFC,IAAI,EAAE,IAAI;YACVC,KAAK,EAAE,IAAI;YACXC,MAAM,EAAE,IAAI;YACZC,YAAY,EAAE;UAClB,CAAC;UACDC,KAAK,EAAE,CACH;YACIN,IAAI,EAAE,UAAU;YAChBf,IAAI,EAAEC,MAAM;YACZqB,QAAQ,EAAE;cACNC,cAAc,EAAE;YACpB;UACJ,EACH;UACDC,KAAK,EAAE,CACH;YACIT,IAAI,EAAE;UACV,EACH;UACDU,MAAM,EAAE,CACJ;YACI/B,IAAI,EAAE,IAAI;YACVqB,IAAI,EAAE,KAAK;YACXW,QAAQ,EAAE,KAAK;YACf1B,IAAI,EAAEE;UACV;QAER,CAAC;QACDK,SAAS,CAACoB,SAAS,CAAChB,SAAS,CAAC;MAClC,CAAC,CAAC,CAACiB,KAAK,CAAEC,KAAK,IAAK;QAChBC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC;MACtB,CAAC;IAEL;IACA;IACAtC,SAA