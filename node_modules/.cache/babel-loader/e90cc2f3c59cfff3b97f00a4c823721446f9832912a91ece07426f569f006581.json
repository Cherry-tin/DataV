{"ast":null,"code":"import axios from 'axios';\nimport * as echarts from 'echarts';\n// 导入生命周期函数\nimport { onMounted } from 'vue';\nexport default {\n  name: 'ChinaMap',\n  setup() {\n    const getdata = () => {\n      // then请求成功，catch请求失败\n      axios.get('http://localhost:8080/china.json').then(res => {\n        // 输出矢量地图数据\n        console.log(res.data);\n        var chinaCharts = echarts.init(document.getElementById('CN_Map'));\n        // 在axios的回调函数中注册地图矢量数据\n        echarts.registerMap('chinaMap', res.data);\n        var CNoption = {\n          geo: {\n            type: 'map',\n            map: 'chinaMap',\n            roam: true,\n            //允许缩放以及拖动效果\n            label: {\n              show: true //显示标签\n            },\n\n            zoom: 1.5,\n            //初始的大小比例\n            // center: [],设置地图中心店的坐标\n\n            //鼠标悬停效果\n            emphasis: {\n              label: {\n                textStyle: {\n                  color: 'pink'\n                } //悬停时字体颜色\n              },\n\n              itemStyle: {\n                areaColor: 'red' //区域颜色\n              }\n            },\n\n            itemStyle: {\n              // 设置边框为白色\n              normal: {\n                borderWidth: 0.5,\n                //边界线大小\n                borderColor: '#fff' //边界线颜色\n              }\n            }\n          },\n\n          series: [{\n            title: {\n              left: center\n            }\n          }]\n        };\n        chinaCharts.setOption(CNoption);\n      }).catch(error => {\n        console.log(error);\n      });\n    };\n    // 调用，执行函数\n    onMounted(() => {\n      getdata();\n    });\n  }\n};","map":{"version":3,"names":["axios","echarts","onMounted","name","setup","getdata","get","then","res","console","log","data","chinaCharts","init","document","getElementById","registerMap","CNoption","geo","type","map","roam","label","show","zoom","emphasis","textStyle","color","itemStyle","areaColor","normal","borderWidth","borderColor","series","title","left","center","setOption","catch","error"],"sources":["D:\\VUE_project\\data_screen\\src\\components\\ChinaMap.vue"],"sourcesContent":["<template>\r\n  <div id=\"CN_Map\" style=\"width: 600px;height: 600px;\"></div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport axios from 'axios';\r\nimport * as echarts from 'echarts'\r\n// 导入生命周期函数\r\nimport { onMounted } from 'vue';\r\nexport default {\r\n  name: 'ChinaMap',\r\n  setup() {\r\n    const getdata = () => {\r\n      // then请求成功，catch请求失败\r\n      axios.get('http://localhost:8080/china.json').then((res) => {\r\n        // 输出矢量地图数据\r\n        console.log(res.data);\r\n        var chinaCharts = echarts.init(document.getElementById('CN_Map'));\r\n        // 在axios的回调函数中注册地图矢量数据\r\n        echarts.registerMap('chinaMap', res.data);\r\n        var CNoption = {\r\n          geo: {\r\n            type: 'map',\r\n            map: 'chinaMap',\r\n            roam: true,  //允许缩放以及拖动效果\r\n            label: {\r\n              show: true  //显示标签\r\n            },\r\n            zoom: 1.5,    //初始的大小比例\r\n            // center: [],设置地图中心店的坐标\r\n\r\n\r\n            //鼠标悬停效果\r\n            emphasis: {\r\n              label: {\r\n                textStyle: { color: 'pink' },//悬停时字体颜色\r\n              },\r\n              itemStyle: {\r\n                areaColor: 'red' //区域颜色\r\n              }\r\n            },\r\n            itemStyle: {\r\n              // 设置边框为白色\r\n              normal: {\r\n                borderWidth: 0.5,//边界线大小\r\n                borderColor: '#fff',//边界线颜色\r\n              }\r\n            }\r\n          },\r\n          series: [\r\n            {\r\n              title:{\r\n                left:center,\r\n                \r\n              } \r\n            }\r\n          ]\r\n        };\r\n        chinaCharts.setOption(CNoption);\r\n      }).catch((error) => {\r\n        console.log(error);\r\n      });\r\n    }\r\n    // 调用，执行函数\r\n    onMounted(() => {\r\n      getdata();\r\n    });\r\n\r\n  }\r\n}\r\n</script>\r\n\r\n<style></style>"],"mappings":"AAMA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAO,KAAKC,OAAM,MAAO,SAAQ;AACjC;AACA,SAASC,SAAQ,QAAS,KAAK;AAC/B,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,KAAKA,CAAA,EAAG;IACN,MAAMC,OAAM,GAAIA,CAAA,KAAM;MACpB;MACAL,KAAK,CAACM,GAAG,CAAC,kCAAkC,CAAC,CAACC,IAAI,CAAEC,GAAG,IAAK;QAC1D;QACAC,OAAO,CAACC,GAAG,CAACF,GAAG,CAACG,IAAI,CAAC;QACrB,IAAIC,WAAU,GAAIX,OAAO,CAACY,IAAI,CAACC,QAAQ,CAACC,cAAc,CAAC,QAAQ,CAAC,CAAC;QACjE;QACAd,OAAO,CAACe,WAAW,CAAC,UAAU,EAAER,GAAG,CAACG,IAAI,CAAC;QACzC,IAAIM,QAAO,GAAI;UACbC,GAAG,EAAE;YACHC,IAAI,EAAE,KAAK;YACXC,GAAG,EAAE,UAAU;YACfC,IAAI,EAAE,IAAI;YAAG;YACbC,KAAK,EAAE;cACLC,IAAI,EAAE,IAAG,CAAG;YACd,CAAC;;YACDC,IAAI,EAAE,GAAG;YAAK;YACd;;YAGA;YACAC,QAAQ,EAAE;cACRH,KAAK,EAAE;gBACLI,SAAS,EAAE;kBAAEC,KAAK,EAAE;gBAAO,CAAC,CAAC;cAC/B,CAAC;;cACDC,SAAS,EAAE;gBACTC,SAAS,EAAE,KAAI,CAAE;cACnB;YACF,CAAC;;YACDD,SAAS,EAAE;cACT;cACAE,MAAM,EAAE;gBACNC,WAAW,EAAE,GAAG;gBAAC;gBACjBC,WAAW,EAAE,MAAM,CAAC;cACtB;YACF;UACF,CAAC;;UACDC,MAAM,EAAE,CACN;YACEC,KAAK,EAAC;cACJC,IAAI,EAACC;YAEP;UACF;QAEJ,CAAC;QACDxB,WAAW,CAACyB,SAAS,CAACpB,QAAQ,CAAC;MACjC,CAAC,CAAC,CAACqB,KAAK,CAAEC,KAAK,IAAK;QAClB9B,OAAO,CAACC,GAAG,CAAC6B,KAAK,CAAC;MACpB,CAAC,CAAC;IACJ;IACA;IACArC,SAAS,CAAC,MAAM;MACdG,OAAO,CAAC,CAAC;IACX,CAAC,CAAC;EAEJ;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}