{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\n// 导入生命周期函数\nimport { onMounted } from 'vue';\nimport axios from 'axios';\nimport * as echarts from 'echarts';\nexport default {\n  name: 'PhySics',\n  setup() {\n    const gender = () => {\n      var Provdata = []; //省分数线\n      var Xhdata = []; //校分数线\n      var arrayx = []; //年\n\n      axios.get('http://localhost:8888/gscorel/list').then(gphy => {\n        // console.log(cam.data);\n        // 处理数据\n        const data = gphy.data;\n        var arrayy = []; //分数\n        // 分类数据\n        for (let item = 0; item < data.length; item++) {\n          arrayx.push(data[item].year);\n          arrayy.push(data[item].score);\n        }\n        // 输入省分数线数据\n        for (var i = 0; i < arrayy.length; i++) {\n          Provdata.push({\n            value: arrayy[i]\n          });\n        }\n      }).catch(error => {\n        console.log(error);\n      });\n      axios.get('http://localhost:8888/xhscorel/list').then(xhphy => {\n        // 处理数据\n        const data = xhphy.data;\n        var arrayy = []; //分数\n        // 分类数据\n        for (let item = 0; item < data.length; item++) {\n          arrayy.push(data[item].score);\n        }\n        // 输入省分数线数据\n        for (var i = 0; i < arrayy.length; i++) {\n          Xhdata.push({\n            value: arrayy[i]\n          });\n        }\n      }).catch(error => {\n        console.log(error);\n      });\n      console.log(Provdata);\n      console.log(Xhdata);\n      var PyhCharts = echarts.init(document.getElementById('physics'));\n      var Pyhoption = {\n        title: {\n          text: 'Stacked Line'\n        },\n        tooltip: {\n          trigger: 'axis'\n        },\n        legend: {\n          data: ['Email', 'Union Ads', 'Video Ads', 'Direct', 'Search Engine']\n        },\n        grid: {\n          left: '3%',\n          right: '4%',\n          bottom: '3%',\n          containLabel: true\n        },\n        toolbox: {\n          feature: {\n         