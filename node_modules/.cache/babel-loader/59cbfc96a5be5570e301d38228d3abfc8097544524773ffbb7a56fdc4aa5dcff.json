{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\n// 导入生命周期函数\nimport { onMounted } from 'vue';\nimport axios from 'axios';\nimport * as echarts from 'echarts';\nexport default {\n  name: 'ChinaMap',\n  setup() {\n    const gender = () => {\n      axios.get('http://localhost:8888/gender/list').then(sex => {\n        console.log(sex.data);\n        // 处理数据\n        const data = sex.data;\n        var GDdata = []; //饼图数据\n        var type = []; //性别\n        var num = []; //人数\n\n        // 分类数据\n        for (let item = 0; item < data.length; item++) {\n          type.push(data[item].type);\n          num.push(data[item].value);\n        }\n        // 输入饼图数据\n        for (var i = 0; i < sex.length; i++) {\n          GDdata.push({\n            type: type[i],\n            number: num[i]\n          });\n        }\n        var GDCharts = echarts.init(document.getElementById('gender'));\n        var GDoption = {\n          tooltip: {\n            trigger: 'item',\n            formatter: '{b}<br/>人数：{c}'\n          },\n          // 鼠标悬停显示数据\n          legend: {\n            top: '5%',\n            left: 'center'\n          },\n          series: [{\n            name: 'Access From',\n            type: 'pie',\n            radius: ['40%', '70%'],\n            avoidLabelOverlap: false,\n            itemStyle: {\n              borderRadius: 10,\n              borderColor: '#fff',\n              borderWidth: 2\n            },\n            label: {\n              show: false,\n              position: 'center'\n            },\n            emphasis: {\n              label: {\n                show: true,\n                fontSize: 40,\n                fontWeight: 'bold'\n              }\n            },\n            labelLine: {\n              show: false\n            },\n            data: GDdata\n          }]\n        };\n        GDCharts.setOption(GDoption);\n      }).catch(error => {\n        console.log(error);\n      });\n    };\n    // 调用，执行函数\n    onMounted(() => {\n      gender();\n    });\n  }\n};","map":{"version":3,"names":["onMounted","axios","echarts","name","setup","gender","get","then","sex","console","log","data","GDdata","type","num","item","length","push","value","i","number","GDCharts","init","document","getElementById","GDoption","tooltip","trigger","formatter","legend","top","left","series","radius","avoidLabelOverlap","itemStyle","borderRadius","borderColor","borderWidth","label","show","position","emphasis","fontSize","fontWeight","labelLine","setOption","catch","error"],"sources":["D:\\VUE_project\\data_screen\\src\\components\\Gender.vue"],"sourcesContent":["<template>\r\n    <div id=\"gender\"></div>\r\n</template>\r\n\r\n<script>\r\n// 导入生命周期函数\r\nimport { onMounted } from 'vue';\r\nimport axios from 'axios';\r\nimport * as echarts from 'echarts'\r\n\r\nexport default {\r\n\r\n    name: 'ChinaMap',\r\n    setup() {\r\n        const gender = () => {\r\n            axios.get('http://localhost:8888/gender/list').then((sex) => {\r\n                console.log(sex.data);\r\n                // 处理数据\r\n                const data = sex.data;\r\n                var GDdata = [];//饼图数据\r\n                var type = [];//性别\r\n                var num = [];//人数\r\n\r\n                // 分类数据\r\n                for (let item = 0; item < data.length; item++) {\r\n                    type.push(data[item].type);\r\n                    num.push(data[item].value);\r\n                }\r\n                // 输入饼图数据\r\n                for (var i = 0; i < sex.length; i++) {\r\n                    GDdata.push({\r\n                        type: type[i],\r\n                        number: num[i]\r\n                    })\r\n                }\r\n                var GDCharts = echarts.init(document.getElementById('gender'));\r\n                var GDoption = {\r\n                    tooltip: {\r\n                        trigger: 'item',\r\n                        formatter: '{b}<br/>人数：{c}'\r\n                    },\r\n                    // 鼠标悬停显示数据\r\n                    legend: {\r\n                        top: '5%',\r\n                        left: 'center'\r\n                    },\r\n                    series: [\r\n                        {\r\n                            name: 'Access From',\r\n                            type: 'pie',\r\n                            radius: ['40%', '70%'],\r\n                            avoidLabelOverlap: false,\r\n                            itemStyle: {\r\n                                borderRadius: 10,\r\n                                borderColor: '#fff',\r\n                                borderWidth: 2\r\n                            },\r\n                            label: {\r\n                                show: false,\r\n                                position: 'center'\r\n                            },\r\n                            emphasis: {\r\n                                label: {\r\n                                    show: true,\r\n                                    fontSize: 40,\r\n                                    fontWeight: 'bold'\r\n                                }\r\n                            },\r\n                            labelLine: {\r\n                                show: false\r\n                            },\r\n                            data: GDdata\r\n                        }\r\n                    ]\r\n                };\r\n                GDCharts.setOption(GDoption);\r\n            }).catch((error) => {\r\n                console.log(error);\r\n            })\r\n\r\n        }\r\n        // 调用，执行函数\r\n        onMounted(() => {\r\n            gender();\r\n        });\r\n    }\r\n\r\n}\r\n</script>\r\n\r\n<style>\r\n#gender {\r\n    width: 800px;\r\n    height: 800px;\r\n}\r\n</style>"],"mappings":";AAKA;AACA,SAASA,SAAQ,QAAS,KAAK;AAC/B,OAAOC,KAAI,MAAO,OAAO;AACzB,OAAO,KAAKC,OAAM,MAAO,SAAQ;AAEjC,eAAe;EAEXC,IAAI,EAAE,UAAU;EAChBC,KAAKA,CAAA,EAAG;IACJ,MAAMC,MAAK,GAAIA,CAAA,KAAM;MACjBJ,KAAK,CAACK,GAAG,CAAC,mCA