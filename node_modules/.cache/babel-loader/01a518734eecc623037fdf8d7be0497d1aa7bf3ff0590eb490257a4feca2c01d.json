{"ast":null,"code":"import axios from 'axios';\nimport * as echarts from 'echarts';\n// 导入生命周期函数\nimport { onMounted } from 'vue';\nexport default {\n  name: 'ChinaMap',\n  setu() {\n    const getdata = () => {\n      // then请求成功，catch请求失败\n      axios.get('../json/china.json').then(res => {\n        // 输出矢量地图数据\n        console.log(res.data);\n        console.log('ho');\n        var chinaCharts = echarts.init.init(document.getElementById('CN_Map'));\n        echarts.registerMap('chinaMap', res);\n        var CNoption = {\n          geo: {\n            type: 'map',\n            map: 'chinaMap'\n          }\n        };\n        chinaCharts.setOption(CNoption);\n      }).catch(error => {\n        console.log(error);\n      });\n    };\n    // 调用，执行函数\n    onMounted(() => {\n      getdata();\n    });\n  }\n};","map":{"version":3,"names":["axios","echarts","onMounted","name","setu","getdata","get","then","res","console","log","data","chinaCharts","init","document","getElementById","registerMap","CNoption","geo","type","map","setOption","catch","error"],"sources":["D:\\VUE_project\\data_screen\\src\\components\\ChinaMap.vue"],"sourcesContent":["<template>\r\n  <div id=\"CN_Map\" style=\"width: 500px;height: 500px;bc\"></div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport axios from 'axios';\r\nimport * as echarts from 'echarts'\r\n// 导入生命周期函数\r\nimport { onMounted } from 'vue';\r\nexport default {\r\n  name: 'ChinaMap',\r\n  setu() {\r\n    const getdata = () => {\r\n      // then请求成功，catch请求失败\r\n      axios.get('../json/china.json').then((res) => {\r\n        // 输出矢量地图数据\r\n        console.log(res.data);\r\n        console.log('ho');\r\n\r\n        var chinaCharts = echarts.init.init(document.getElementById('CN_Map'));\r\n        echarts.registerMap('chinaMap', res);\r\n        var CNoption = {\r\n          geo: {\r\n            type: 'map',\r\n            map: 'chinaMap'\r\n          }\r\n        }\r\n        chinaCharts.setOption(CNoption);\r\n      }).catch((error) => {\r\n        console.log(error);\r\n      });\r\n    }\r\n    // 调用，执行函数\r\n    onMounted(() => {\r\n      getdata();\r\n    });\r\n\r\n  }\r\n}\r\n</script>\r\n\r\n<style></style>"],"mappings":"AAMA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAO,KAAKC,OAAM,MAAO,SAAQ;AACjC;AACA,SAASC,SAAQ,QAAS,KAAK;AAC/B,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACL,MAAMC,OAAM,GAAIA,CAAA,KAAM;MACpB;MACAL,KAAK,CAACM,GAAG,CAAC,oBAAoB,CAAC,CAACC,IAAI,CAAEC,GAAG,IAAK;QAC5C;QACAC,OAAO,CAACC,GAAG,CAACF,GAAG,CAACG,IAAI,CAAC;QACrBF,OAAO,CAACC,GAAG,CAAC,IAAI,CAAC;QAEjB,IAAIE,WAAU,GAAIX,OAAO,CAACY,IAAI,CAACA,IAAI,CAACC,QAAQ,CAACC,cAAc,CAAC,QAAQ,CAAC,CAAC;QACtEd,OAAO,CAACe,WAAW,CAAC,UAAU,EAAER,GAAG,CAAC;QACpC,IAAIS,QAAO,GAAI;UACbC,GAAG,EAAE;YACHC,IAAI,EAAE,KAAK;YACXC,GAAG,EAAE;UACP;QACF;QACAR,WAAW,CAACS,SAAS,CAACJ,QAAQ,CAAC;MACjC,CAAC,CAAC,CAACK,KAAK,CAAEC,KAAK,IAAK;QAClBd,OAAO,CAACC,GAAG,CAACa,KAAK,CAAC;MACpB,CAAC,CAAC;IACJ;IACA;IACArB,SAAS,CAAC,MAAM;MACdG,OAAO,CAAC,CAAC;IACX,CAAC,CAAC;EAEJ;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}