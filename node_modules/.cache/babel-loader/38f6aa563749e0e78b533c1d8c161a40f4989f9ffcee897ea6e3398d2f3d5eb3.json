{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nimport * as echarts from 'echarts';\n// 导入生命周期函数\nimport { onMounted } from 'vue';\nexport default {\n  name: 'ChinaMap',\n  setup() {\n    const getdata = () => {\n      // 录取数据\n      var Cdata = [];\n      var mapdata;\n      axios.get('http://localhost:8888/chinamap/list').then(snum => {\n        // 输出录取人数数据\n        console.log(snum.data);\n        // 处理数据\n        const data = snum.data;\n        var area = [];\n        var sumb = [];\n        // 用循环遍历数据\n        for (var item = 0; item < data.length; item++) {\n          area.push(data[item].name);\n          sumb.push(data[item].value);\n        }\n        // 录取数据\n        for (var i = 0; i < area.length; i++) {\n          Cdata.push({\n            value: sumb[i],\n            names: area[i]\n          });\n        }\n      }).catch(error => {\n        console.log(error);\n      });\n      // then请求成功，catch请求失败\n      axios.get('http://localhost:8080/china.json').then(res => {\n        // 输出矢量地图数据\n        console.log(res.data);\n        // 储存地图数据\n        mapdata = res.data;\n      }).catch(error => {\n        console.log(error);\n      });\n    };\n    // 调用，执行函数\n    onMounted(() => {\n      getdata();\n    });\n  }\n};","map":{"version":3,"names":["axios","echarts","onMounted","name","setup","getdata","Cdata","mapdata","get","then","snum","console","log","data","area","sumb","item","length","push","value","i","names","catch","error","res"],"sources":["D:\\VUE_project\\data_screen\\src\\components\\ChinaMap.vue"],"sourcesContent":["<template>\r\n  <div id=\"CN_Map\" style=\"width: 800px;height: 800px;\"></div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport axios from 'axios';\r\nimport * as echarts from 'echarts'\r\n// 导入生命周期函数\r\nimport { onMounted } from 'vue';\r\nexport default {\r\n  name: 'ChinaMap',\r\n  setup() {\r\n    const getdata = () => {\r\n      // 录取数据\r\n      var Cdata = [];\r\n      var mapdata;\r\n      axios.get('http://localhost:8888/chinamap/list').then((snum) => {\r\n        // 输出录取人数数据\r\n        console.log(snum.data);\r\n        // 处理数据\r\n        const data = snum.data\r\n        var area = [];\r\n        var sumb = [];\r\n        // 用循环遍历数据\r\n        for (var item = 0; item < data.length; item++) {\r\n          area.push(data[item].name);\r\n          sumb.push(data[item].value);\r\n        }\r\n        // 录取数据\r\n        for (var i = 0; i < area.length; i++) {\r\n          Cdata.push({\r\n            value: sumb[i],\r\n            names: area[i]\r\n          })\r\n        }\r\n      }).catch((error) => {\r\n        console.log(error);\r\n      });\r\n      // then请求成功，catch请求失败\r\n      axios.get('http://localhost:8080/china.json').then((res) => {\r\n        // 输出矢量地图数据\r\n        console.log(res.data);\r\n        // 储存地图数据\r\n        mapdata = res.data;\r\n      }).catch((error) => {\r\n        console.log(error);\r\n      });\r\n     \r\n    }\r\n    // 调用，执行函数\r\n    onMounted(() => {\r\n      getdata();\r\n    });\r\n\r\n  }\r\n}\r\n</script>\r\n\r\n<style></style>"],"mappings":";AAMA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAO,KAAKC,OAAM,MAAO,SAAQ;AACjC;AACA,SAASC,SAAQ,QAAS,KAAK;AAC/B,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,KAAKA,CAAA,EAAG;IACN,MAAMC,OAAM,GAAIA,CAAA,KAAM;MACpB;MACA,IAAIC,KAAI,GAAI,EAAE;MACd,IAAIC,OAAO;MACXP,KAAK,CAACQ,GAAG,CAAC,qCAAqC,CAAC,CAACC,IAAI,CAAEC,IAAI,IAAK;QAC9D;QACAC,OAAO,CAACC,GAAG,CAACF,IAAI,CAACG,IAAI,CAAC;QACtB;QACA,MAAMA,IAAG,GAAIH,IAAI,CAACG,IAAG;QACrB,IAAIC,IAAG,GAAI,EAAE;QACb,IAAIC,IAAG,GAAI,EAAE;QACb;QACA,KAAK,IAAIC,IAAG,GAAI,CAAC,EAAEA,IAAG,GAAIH,IAAI,CAACI,MAAM,EAAED,IAAI,EAAE,EAAE;UAC7CF,IAAI,CAACI,IAAI,CAACL,IAAI,CAACG,IAAI,CAAC,CAACb,IAAI,CAAC;UAC1BY,IAAI,CAACG,IAAI,CAACL,IAAI,CAACG,IAAI,CAAC,CAACG,KAAK,CAAC;QAC7B;QACA;QACA,KAAK,IAAIC,CAAA,GAAI,CAAC,EAAEA,CAAA,GAAIN,IAAI,CAACG,MAAM,EAAEG,CAAC,EAAE,EAAE;UACpCd,KAAK,CAACY,IAAI,CAAC;YACTC,KAAK,EAAEJ,IAAI,CAACK,CAAC,CAAC;YACdC,KAAK,EAAEP,IAAI,CAACM,CAAC;UACf,CAAC;QACH;MACF